GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LBBlurredImage" "${PODS_ROOT}/Headers/Public/Mantle"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/LBBlurredImage" "$PODS_CONFIGURATION_BUILD_DIR/Mantle"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LBBlurredImage" -isystem "${PODS_ROOT}/Headers/Public/Mantle"
OTHER_LDFLAGS = $(inherited) -ObjC -l"LBBlurredImage" -l"Mantle" -framework "Accelerate" -framework "CoreGraphics" -framework "Foundation" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
